autogen definitions attributes;


chars = {
   char		= "\\002";
   name		= "Bold";
   description	= "Print text in boldface (some terminals may simply brighten "
                  "the colour of the text instead)";
   langtag_code	= "b";
};

chars = {
   char		= "\\003";
   name		= "Colour";
   description	= "Colour changing sequence";
   langtag_code	= "c";
};

chars = {
   char		= "\\006";
   name		= "Flash";
   description	= "Flash text";
   langtag_code	= "f";
};

chars = {
   char		= "\\007";
   name		= "Beep";
   description	= "Emit a beep or play a predefined sound file";
   langtag_code	= "g";
};

chars = {
   char		= "\\017";
   name		= "Reset";
   description	= "Turn off or reset all text attributes";
   langtag_code	= "o";
};

chars = {
   char		= "\\026";
   name		= "Reverse";
   description	= "Reverse background/foreground colours (sometimes this will "
                  "only toggle black on white/white on black text)";
   langtag_code	= "r";
};

chars = {
   char		= "\\037";
   name		= "Underline";
   description	= "Underline text (some terminals may italicise, discolour, "
                  "or enbold the text)";
   langtag_code	= "u";
};
