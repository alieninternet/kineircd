autogen definitions errors;


errors = {
   name		= "CLIENT_EXISTS";
   reason	= "Client already in list";
};

errors = {
   name		= "NETWORK_EXISTS";
   reason	= "Network already in list";
};

errors = {
   name		= "NICKNAME_HAS_BAD_CHARS";
   reason	= "The nickname given has invalid characters";
};

errors = {
   name		= "NICKNAME_IS_IN_USE";
   reason	= "The nickname given is already in use";
};

errors = {
   name		= "NICKNAME_TOO_LONG";
   reason	= "The nickname given is too long to be acceptable";
};

errors = {
   name		= "SERVER_EXISTS";
   reason	= "Server already in list";
};

errors = {
   name		= "SERVICE_EXISTS";
   reason	= "Service already in list";
};

errors = {
   name		= "UNREGISTERED_ENTITY";
   reason	= "Operation could not be completed - the given entity is not "
                  "registered";
};

errors = {
   name		= "UNSUPPORTED_BY_ENTITY";
   reason	= "Cannot complete action, it is not supported by the entity";
};

errors = {
   name		= "PERMISSION_DENIED";
   reason	= "Permission denied";
};
